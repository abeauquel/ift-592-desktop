{
  "name": "athlimage",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.1.10",
    "electron-builder": "^22.8.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "bootstrap": "^5.1.3",
    "electron-packager": "^15.4.0",
    "electron-sticky-shim": "gist:4c7dd81a61adcb648944a16b0fac717f",
    "electron-store": "^8.0.1",
    "electron-window-state": "^5.0.3",
    "jquery": "^3.6.0",
    "js-datepicker": "^5.18.0",
    "popper": "^1.0.1",
    "popper.js": "^1.16.1",
    "yarn": "^1.22.17"
  },
  "build": {
    "appId": "athlimage",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/icon/win.ico",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "installerIcon": "/icons/Athlimage-logo.png",
      "uninstallerIcon": "src/icon/win.ico",
      "uninstallDisplayName": "downloadClient",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
